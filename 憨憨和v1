#include<stdio.h>
void main()
{
	char s[] = "1+2+2+1+2+5+4-1-3+4-8";
	int a = s[0] - '0';
	int b = s[2] - '0';
	int c= s[4] - '0';
	int d = s[6] - '0';
	int e = s[8] - '0';
	int f = s[10] - '0';
	int g = s[12] - '0';
	int h = s[14]- '0';
	int i = s[16] - '0';
	int j = s[18] - '0';
	int k = s[20] - '0';
	printf("%d\n", a + b+c+d+e+f+g-h-i+j-k);
}
